{
  "name": "hermesjs",
  "version": "0.1.0",
  "description": "Typescript Web Server framework built on top of μWebSockets.js library (https://github.com/uNetworking/uWebSockets.js)",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint src/**/*.ts --quiet",
    "build:js": "rollup -c",
    "build:pre-clean": "rimraf ./dist",
    "build": "npm run build:pre-clean && npm run build:js",
    "postinstall": "if [ ! -e .no-postinstall ]; then node ./post_install_script.js; fi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miguel-pm/hermesjs.git"
  },
  "keywords": [
    "TypeScript",
    "μWebSockets.js",
    "Hermes.js"
  ],
  "author": "MiguelPM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/miguel-pm/hermesjs/issues"
  },
  "homepage": "https://github.com/miguel-pm/hermesjs#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-dts": "^1.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v17.1.0"
  }
}
