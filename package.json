{
  "name": "hermesjs",
  "version": "1.0.0",
  "description": "Typescript Web Server framework built on top of μWebSockets.js library (https://github.com/uNetworking/uWebSockets.js)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint src/**/*.ts --quiet",
    "build:js": "./node_modules/.bin/tsc",
    "build:pre-clean": "./node_modules/.bin/rimraf ./dist",
    "build": "npm run build:pre-clean && npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miguel-pm/hermesjs.git"
  },
  "keywords": [
    "TypeScript",
    "μWebSockets.js",
    "Hermes.js"
  ],
  "author": "MiguelPM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/miguel-pm/hermesjs/issues"
  },
  "homepage": "https://github.com/miguel-pm/hermesjs#readme",
  "devDependencies": {
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v17.1.0"
  }
}
