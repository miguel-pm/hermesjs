{
  "name": "@miguel-pm/hermes.js",
  "version": "1.1.0",
  "description": "Typescript Web Server framework built on top of μWebSockets.js library (https://github.com/uNetworking/uWebSockets.js)",
  "main": "./dist/hermes.js",
  "types": "./dist/hermes.d.ts",
  "module": "./dist/hermes.es.js",
  "scripts": {
    "lint": "eslint src/**/*.ts --quiet",
    "build:transpile-ts": "./node_modules/.bin/rollup -c",
    "prebuild": "./node_modules/.bin/rimraf ./dist",
    "build": "npm run lint && npm run build:transpile-ts",
    "prepublish": "npm run build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist/",
    "README.md",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miguel-pm/hermesjs.git"
  },
  "keywords": [
    "TypeScript",
    "μWebSockets.js",
    "Hermes.js"
  ],
  "author": "MiguelPM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/miguel-pm/hermesjs/issues"
  },
  "homepage": "https://github.com/miguel-pm/hermesjs#readme",
  "dependencies": {
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v17.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^1.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
